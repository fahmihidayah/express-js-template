{
  "name": "blog-clean-api",
  "version": "0.0.0",
  "description": "TypeScript + Prisma + MySQL + Express API Server",
  "author": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "devold": "nodemon -r tsconfig-paths/register src/index.ts",
    "prisma:init": "prisma init",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:generate": "prisma generate",
    "build": "tsc",
    "lint": "eslint --fix src test",
    "test": "jest --forceExit --detectOpenHandles",
    "test:unit": "ENV_FILE=.env.development.local jest",
    "typecheck": "tsc --noEmit",
    "test:ci": "DATABASE_URL=\"file:./test.db\" jest",
    "migrate:test:ci": "DATABASE_URL=\"file:./test.db\" prisma migrate dev"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/w3cj/express-api-starter.git"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "^6.4.3",
    "jest-mock-extended": "^3.0.4",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "sqlite": "^5.0.1",
    "sqlite3": "^5.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@quramy/jest-prisma": "^1.5.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.69",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "4.2.1",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "chai": "4.2.0",
    "cross-env": "7.0.3",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "node-config": "^0.0.2",
    "node-gyp": "^9.4.0",
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
